{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "ColorAgent",
    "extends": "./tsconfig.base.json",
    "compilerOptions": {
        "composite": true,
        "incremental": true,
        "tsBuildInfoFile": "tsconfig.native.tsbuildinfo",
        "jsx": "react-native",
        "jsxImportSource": "react",
        "lib": ["DOM", "ESNext"],
        "noEmit": true
    }
}
